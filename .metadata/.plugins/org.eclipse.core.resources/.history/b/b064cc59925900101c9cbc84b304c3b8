package java_fundamentals;

import java.util.Scanner;
public class Question19 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		for (int i = 1; i<=num; i++) {
			for(int j = num; j<=num; j++) {
				System.out.println("*");
			}
		}
	}

}
